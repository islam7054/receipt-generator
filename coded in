<script type="text/javascript" language="javascript">
    $(function () {
        var policySecurityHelper = new Tc.PolicySecurityHelperConstructor();
        policySecurityHelper.BindDateControlsChanged();
        policySecurityHelper.formfocus();
    });

    $(document).ready(function() {
        $.validator.unobtrusive.parse(document);

        $('#PolicySecurityCheckFrm').on('submit', function(event) {
            if (!$(this).valid()) {
                event.preventDefault();
                $(this).find(':input').trigger('blur');
            }
        });
    });


    function generateRandomNumber() {
        var $input = $('#product_model');

        var randomNumber = Math.floor(Math.random() * 90000000) + 10000000;
        var policyNumber = "TCV-MOT-" + randomNumber;
        $input.val(policyNumber);
        $input.trigger('blur');
    }
</script>
